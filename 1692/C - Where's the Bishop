#include <bits/stdc++.h>using namespace std; void solve(){    // LOGIC        char v[8][8];     for(int i = 0; i < 8; i++){        for(int j = 0; j < 8; j++){            cin >> v[i][j];        }    }     for(int i = 1; i < 7; i++){        for(int j = 1; j < 7; j++){            if(v[i][j] == '#' && v[i - 1][j - 1] == '#' && v[i - 1][j + 1] && v[i + 1][j - 1] == '#' && v[i + 1][j + 1] == '#'){                cout << i + 1 << " " << j + 1 << endl;                return;            }        }    } } int main(){    ios::sync_with_stdio(false); // Fast I/O    cin.tie(nullptr);            // Untie cin from cout     int n;    cin >> n;    while(n--) solve();}