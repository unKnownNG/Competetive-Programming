#include <bits/stdc++.h>using namespace std; void solve(){    // LOGIC    int a,b,c,d;    set<int> s;    cin >> a >> b >> c >> d;    s.insert(a);    s.insert(b);    s.insert(c);    s.insert(d);    cout << (s.size()== 1 ? "YES" : "NO") << endl;} int main(){    ios::sync_with_stdio(false); // Fast I/O    cin.tie(nullptr);            // Untie cin from cout     int n;    cin >> n;    while(n--) solve();}